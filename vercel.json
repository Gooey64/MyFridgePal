{
        "version": 2,
        "builds": [
          {
            "src": "backend/server.js",
            "use": "@vercel/node"
          }
        ],
        "routes": [
          {
            "src": "/",
            "dest": "/Frontend/login.html"
          },
          {
            "src": "/(.*)",
            "dest": "/Frontend/$1"
          }
        ]
      }
      